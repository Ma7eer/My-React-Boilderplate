{
  "name": "react-boilderplate",
  "version": "0.0.1",
  "description": "My own react boilderplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "test": "jest ./src",
    "clean": "rimraf dist node_modules",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ma7eer/My-React-Boilderplate.git"
  },
  "keywords": [
    "node",
    "react",
    "babel",
    "Webpack"
  ],
  "author": "Maher Alkendi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ma7eer/My-React-Boilderplate/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js"
  },
  "homepage": "https://github.com/Ma7eer/My-React-Boilderplate#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.5.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.3",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}

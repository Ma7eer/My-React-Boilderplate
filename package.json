{
  "name": "react-boilderplate",
  "version": "0.0.1",
  "description": "My own react boilderplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "jest .src/ && npm run build && node ./dist/index.js",
    "test": "jest .src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ma7eer/My-React-Boilderplate.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "Maher Alkendi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ma7eer/My-React-Boilderplate/issues"
  },
  "homepage": "https://github.com/Ma7eer/My-React-Boilderplate#readme",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0"
  }
}
